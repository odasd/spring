package MysqlTest;

@RunWith(SpringJUnit4ClassRunner.class) //먼저 SpringJUnit4ClassRunner.class import한다. @ContextConfiguration(locations={"file:src/main/webapp/WEB-INF/spring/**/*.xml"})
public class MysqlTest {
 @Autowired
 private MysqlMapper mapper;
 @Test
 public void getTime() { mapper.getTime(); }
 @Test
 public void getTime2() { mapper.getTime2(); }
}
